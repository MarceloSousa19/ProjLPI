                        captchaHolder.setAttribute('id',captchaHolder.id+'-'+formsFound[formIDForm.id]['counter']);

                        if (typeof formIDForm != "undefined" && formIDForm !== null) {
                            formIDForm.addEventListener('click', reCaptchaOnFormClick, false);
